{
  "multipart": [
    {
      "when": { "down": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder" }
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder", "x": 180 }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder", "x": 90, "y": 180 }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder", "x": 90, "y": 0 }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder", "x": 90, "y": 270 }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "retrocomputers:block/wire_placeholder", "x": 90, "y": 90 }
    }
  ]
}
